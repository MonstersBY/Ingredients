extends pug/base

prepend vars
	//-
	- title = "Ingredients | Продукты"

append vars
	//-

block content
	include pug/components/header-log
	main

		section.section.cabinet.card_product
			include pug/components/search_modal

			.lines
				img(src="images/lines.svg" alt="lines.svg")
			.container
				.sidebar__container
					.sidebar.gradient_border
						.sidebar__top
							.sidebar__img
								img(src="images/cabinet/profile_img.png" alt="profile_img")
							.sidebar__suptitle Компания
							.sidebar__title ООО «Милорада»

						.sidebar__items

							.item
								a.item__btn(href="#")
									.item__icon
										include pug/svg/cabinet/award.svg
									.item__name Бренды
							.item
								a.item__btn(href="#")
									.item__icon
										include pug/svg/cabinet/box.svg
									.item__name Продукты
									.item__number 13
							.item
								a.item__btn(href="#")
									.item__icon
										include pug/svg/cabinet/document-text.svg
									.item__name Документы
									.item__number 13
							.item
								a.item__btn(href="#")
									.item__icon
										include pug/svg/cabinet/profile_2user.svg
									.item__name Эксперты
							.item
								a.item__btn(href="#")
									.item__icon
										include pug/svg/cabinet/lamp-on.svg
									.item__name Разработки
					.sidebar-experts.desc.gradient_border
						.sidebar-experts__top
							.sidebar-experts__icon
								include pug/svg/profile_input.svg
							.sidebar-experts__text Спросите экспертов компании
						.sidebar-experts__bottom
							.sidebar-experts__img
								img(src="images/Group-5295.png" alt="")
							a.sidebar-experts__link.underline(href="#") Написать нам
				.cabinet__content
					.section__title Продукты
					form.search.cabinet__search.twoinput
						.form__item
							select(name="")
								option По функциям
								option По функциям 2
								option По функциям 3
						.label-container
							label
								include ./pug/svg/search_grey.svg
								input(type="text" placeholder="Введите название")
							button(type="submit")
								include ./pug/svg/arrow.svg
					.section__subtitle Соответствующие категории

					.card_product-category
						.card_product-category__item.gradient_border
							.card_product-category__img
								img(src="images/cards/card02.jpg" alt="")
							.card_product-category__title Снэки и злаки
							.card_product-category__props
								.card_product-category__prop Безалкогольные алкогольные
								.card_product-category__prop Алкогольные
								.card_product-category__prop Ферментированные
							a.card_product-category__btn.underline(href="#") Перейти
						.card_product-category__item.gradient_border
							.card_product-category__img
								img(src="images/cards/card03.jpg" alt="")
							.card_product-category__title Напитки
							.card_product-category__props
								.card_product-category__prop Безалкогольные алкогольные
								.card_product-category__prop Алкогольные
								.card_product-category__prop Ферментированные
							a.card_product-category__btn.underline(href="#") Перейти
						.card_product-category__item.gradient_border
							.card_product-category__img
								img(src="images/cards/card04.jpg" alt="")
							.card_product-category__title Кондитерские продукты
							.card_product-category__props
								.card_product-category__prop Безалкогольные алкогольные
								.card_product-category__prop Алкогольные
								.card_product-category__prop Ферментированные
							a.card_product-category__btn.underline(href="#") Перейти

					.sidebar-experts.mobile.gradient_border
